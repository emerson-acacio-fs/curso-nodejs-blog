<%- include('../../partials/header.ejs'); %>
<body>
  <%- include('../../partials/navbar.ejs'); %>
  <div class="container">
    <hr />
    <h2>Usuários</h2>
    <a class="btn btn-success" href="/admin/users/create">Criar novo usuário</a>
    <hr />
    <table class="table table-bordered">
      <thead>
        <tr>
          <th>id</th>
          <th>e-mail</th>
        </tr>
      </thead>
      <tbody>
        <% users.forEach((user)=>{ %>
        <tr>
          <th><%= user.id %></th>
          <th><%= user.email %></th>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </div>
</body>
<%- include('../../partials/footer.ejs') %>
